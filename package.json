{
    "name": "colourant",
    "version": "0.0.0",
    "description": "A Node.js library for formatting terminal text using ANSI",
    "keywords": [
        "ansi",
        "color",
        "format",
        "log",
        "terminal",
        "text"
    ],
    "repository": "futagoza/colourant",
    "license": "MIT",
    "author": "Futago-za Ryuu <futagoza.ryuu@gmail.com>",
    "files": [
        "index.js",
        "index.d.ts"
    ],
    "main": "index.js",
    "types": "index.d.ts",
    "scripts": {
